{
  "multipart": [
    {
      "apply": [
        {
          "model": "atmospheric:block/snowy_bamboo1_age0"
        },
        {
          "model": "atmospheric:block/snowy_bamboo2_age0"
        },
        {
          "model": "atmospheric:block/snowy_bamboo3_age0"
        },
        {
          "model": "atmospheric:block/snowy_bamboo4_age0"
        }
      ],
      "when": {
        "age": "0"
      }
    },
    {
      "apply": [
        {
          "model": "atmospheric:block/snowy_bamboo1_age1"
        },
        {
          "model": "atmospheric:block/snowy_bamboo2_age1"
        },
        {
          "model": "atmospheric:block/snowy_bamboo3_age1"
        },
        {
          "model": "atmospheric:block/snowy_bamboo4_age1"
        }
      ],
      "when": {
        "age": "1"
      }
    },
    {
      "apply": {
        "model": "atmospheric:block/snowy_bamboo_small_leaves"
      },
      "when": {
        "leaves": "small"
      }
    },
    {
      "apply": {
        "model": "atmospheric:block/snowy_bamboo_large_leaves"
      },
      "when": {
        "leaves": "large"
      }
    }
  ]
}